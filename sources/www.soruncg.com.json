{
    "name": "零点看书",
    "url": "www.soruncg.com",
    "version": 100,
    "search": {
        "url": "http://www.soruncg.com/ar.php?keyWord=${key}",
        "list": "tr:nth-child(n+2)",
        "name": "td:nth-child(1)",
        "author": "td:nth-child(3)",
        "detail": "td:nth-child(1) > a"
    },
    "detail": {
        "cover": "#fmimg > img",
        "summary": "#intro > p:nth-child(1)",
        "update": "#info > p:nth-child(4)@match->(?<=最后更新 :).+"

    },
    "catalog": {
        "list": "#list > dl > dd",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#content"
    }
}
