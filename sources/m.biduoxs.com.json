{
    "name": "笔趣阁（biduoxs）",
    "url": "m.biduoxs.com",
    "version": 101,
    "search": {
        "url": "https://m.biduoxs.com/search.php?q=${key}",
        "charset": "utf-8",
        "list": "div.result-list > div.result-item.result-game-item",
        "name": "div.result-game-item-detail>h3",
        "author": "div.result-game-item-detail>div>p:eq(0)>span:eq(1)",
        "detail": "div.result-game-item-detail>h3>a@attr->href@replace->m.biduoxs->www.biduoxs",
        "cover": "div.result-game-item-pic>a>img@attr->src"
    },
    "detail": {
        "name": "#info > h1",
        "status": "#info > p:nth-child(1)",
        "cover": "#fmimg > img",
        "lastChapter": "#info > p:nth-child(3)"
    },
    "catalog": {
        "list": "#list > dl > dd",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#content"
    }
}
